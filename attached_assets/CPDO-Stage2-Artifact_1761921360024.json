      "section": "10 · Workflows",
      "subsections": [
        {
          "title": "10.1 Workflow Catalog",
          "workflows": [
            {
              "workflow_name": "Lead Capture",
              "description": "Create Lead with minimal required fields; validate; save; show confirmation.",
              "input": "Lead form",
              "process": "Validation → Persist → Timeline note",
              "output": "Lead record"
            },
            {
              "workflow_name": "Lead Conversion",
              "description": "Convert Lead into Account, Contact, and initial Opportunity; check for duplicates.",
              "input": "Lead record",
              "process": "Pre-checks → Create related records → Link & close Lead",
              "output": "Account/Contact/Opportunity"
            },
            {
              "workflow_name": "Opportunity Progression",
              "description": "Drag Opportunity across stages on Kanban; set probability; log activity.",
              "input": "Opportunity card",
              "process": "Stage change → Persist → Recalc pipeline",
              "output": "Updated Opportunity"
            },
            {
              "workflow_name": "Activity Logging",
              "description": "Log calls, emails, tasks; auto-stamp user and time; relate to parent.",
              "input": "Activity form",
              "process": "Validation → Persist → Timeline",
              "output": "Activity record"
            },
            {
              "workflow_name": "Admin Backup/Restore",
              "description": "Run backup to storage; restore from selected snapshot.",
              "input": "Admin console action",
              "process": "Dump → Encrypt → Store / Restore → Restart",
              "output": "Backup file / Restored DB"
            },
            {
              "workflow_name": "ID Pattern Configuration",
              "description": "Configure per-entity ID format and starting value; test issuance.",
              "input": "Admin config form",
              "process": "Validate pattern → Save → Apply to issuance service",
              "output": "Active ID pattern"
            }
          ]
        }
      ]
    },
