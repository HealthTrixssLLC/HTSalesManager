{
  "entity_type": "leads",
  "sheet_name": "All Leads",
  "target_template": "dynamics_leads_template.csv",
  "column_mapping": {
    "(Do Not Modify) Lead": "externalId",
    "First Name": "firstName",
    "Last Name": "lastName",
    "Company Name": "company",
    "Email": "email",
    "Business Phone": "phone",
    "Topic": "topic",
    "Status Reason": "status",
    "Lead Source": "source"
  },
  "type_mapping": {
    "status": {
      "New": "new",
      "Contacted": "contacted",
      "Qualified": "qualified",
      "Unqualified": "unqualified",
      "Converted": "converted"
    },
    "source": {
      "Web Site": "website",
      "Referral": "referral",
      "Phone Call": "phone",
      "Email": "email",
      "Event": "event",
      "Partner": "partner",
      "Other": "other"
    }
  },
  "id_rules": {
    "internal_id_field": "id",
    "internal_id_pattern": "LEAD-{{000001}}",
    "external_id_fields": [
      "externalId"
    ],
    "preserve_external_format": false
  },
  "governance_fields": {
    "sourceSystem": "Dynamics 365",
    "importStatus": "imported"
  },
  "validation_rules": {
    "required_fields": ["firstName", "lastName"],
    "email_fields": ["email"],
    "phone_fields": ["phone"],
    "url_fields": [],
    "state_fields": [],
    "postal_fields": []
  },
  "dedupe_rules": {
    "primary_key": ["email"],
    "fuzzy_match_threshold": 90
  }
}
